{
  "name": "gifvcat",
  "version": "0.0.2",
  "manifest_version": 2,
  "description": "Replaces imgur and gfycat gif links with corresponding HTML5 video links",
  "homepage_url": "http://nelad.us",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "js/jquery/jquery.min.js",
        "src/inject/gifvcat.js"
      ]
    }
  ]
}
